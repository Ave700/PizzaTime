<h1>Pizza Time</h1>
<p>This is the catalog of all possible items</p>
<div class="content" id="main">
  <div class="catalog">
    {{#with wrapper}}
        {{#with result.[0]}}
            {{#each results}}
                <div class="topContainer">
                <table>
                    <tr>
                        <th>Topping</th>
                        <th>Price</th>
                    </tr>
                    <tr>
                        <td>{{name}}</td>
                        <td>{{price}}</td>
                    </tr>
                    
                </table>
                </div>
          {{/each}}
        {{/with}}
        

    {{/with}}
  </div>
</div>
<footer>Created for CS340 class</footer>