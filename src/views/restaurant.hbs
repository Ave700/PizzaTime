<div class="nav-bar">
  <a href="/">Home</a>
  <a href="catalog">Catalog</a>
  <a class = "active" href="restaurants">Restaurants</a>
  <a href="locations">Locations</a>
  <a href="login" id="login">Sign In</a>
</div>
<h1>Pizza Time</h1>
<p>Welcome to Pizza Time! A helpful app to find the pizza near you!</p>
<div class="content" id="main">
  <div class="restaurants">
    {{#with wrapper}}
        {{#each objects}}
          <div class="restContainer">
          <h2 id="Rname">{{rest_name}}</h2>
          <p id="Address">{{Address}}</p>
          <p id="phone#">{{PhoneNumber}}</p>
          <p id="zipcode">{{Zipcode}}</p>
          <img src="src/public/images/{{imageName}}.png">
          </div>
        {{/each}}

    {{/with}}
  </div>
</div>
<footer>Created for CS340 class</footer>
